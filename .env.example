# 环境变量配置示例
# 复制此文件为 .env 并填入实际值

# 环境
ENVIRONMENT=test

# API 配置
API_BASE_URL=https://api.example.com
API_TIMEOUT=30

# 认证信息
API_KEY=your_api_key_here
API_SECRET=your_api_secret_here

# 测试用户
TEST_USERNAME=test_user
TEST_PASSWORD=test_password

# 浏览器配置
BROWSER_TYPE=chromium
HEADLESS=false

# 报告配置
REPORT_DIR=reports
